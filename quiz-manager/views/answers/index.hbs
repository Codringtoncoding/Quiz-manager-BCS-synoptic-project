<h1>All answers</h1>
<table>
    <thead>
        <th>ID</th>
        <th>answer</th>
        <th>Correct answer</th>
        <th>Question id it belongs too</th>


    </thead>
    {{#each answers}}
        <tr>
            <td>{{this.id}}</td>
            <td>{{this.answer}}</td>
            <td>{{this.correct}}</td>
            <td>{{this.questionid}}</td>
            <td>
                <button>Go</button>
            </td>
        </tr>
    {{/each}}
</table>